[version]
0.84.0

[ignore]
<PROJECT_ROOT>/dist/.*
<PROJECT_ROOT>/es/.*
<PROJECT_ROOT>/lib/.*

[lints]
all=warn
unclear-type=off
sketchy-null-bool=off
untyped-import=off
sketchy-null-string=off
sketchy-null-number=off
unsafe-getters-setters=off
inexact-spread=off
unnecessary-invariant=off

[options]
include_warnings=true
